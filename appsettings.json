{
    "spring": {
        "application": {
            "name": "cloud-native-dotnet"
        },
        "cloud": {
            "config": {
                "uri": "http://config:8888"
            }
        }
    },
    "eureka": {
        "client": {
            "serviceUrl": "http://eureka:8761/eureka/",
            "shouldRegisterWithEureka": true
        },
        "instance": {
            "preferIpAddress": true,
            "port": 5000
        }
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    }
}
